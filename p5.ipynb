   "source": [
    "import math\n",
    "\n",
    "def smallest_multiple(n):\n",
    "    lcm = 1\n",
    "    for i in range(1, n + 1):\n",
    "        lcm = (lcm * i) // math.gcd(lcm, i)\n",
    "    return lcm"
   ]
